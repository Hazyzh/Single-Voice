{
  "name": "Single Voice",
  "description" : "Let only one tab has voice",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "images/logo.png"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  }
}